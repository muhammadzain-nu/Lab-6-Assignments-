#include <stdio.h>
#include<stdlib.h>
int main(){
	int a, b, totalsum=0;
	int count=0;
	while(count==0){
		printf("Enter Two numbers");
		scanf("%d%d", &a, &b);
	        if(a==0 || b==0){
				printf("Number is zero");
				count++;
			}
			else{
			
			totalsum+=a+b; 
			printf("The Sum is %d", totalsum);
		}
			
			
		return 0;
		
		
		
		
	}
	
