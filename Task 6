#include <stdio.h>
int main(){
int number, reversed=0, remainder;
	printf("Enter a number:");
	scanf("%d", &number);
if(number<0){
	printf("Number is negative");
	
}else{
	while(number!=0){
    remainder=number%10;
    reversed=reversed*10+remainder;    
    number=number/10;
	
	
	
	
}
	
	printf("The reversed number is: %d\n", reversed);
	return 0;
}
	
	
	
	
	
	
}
